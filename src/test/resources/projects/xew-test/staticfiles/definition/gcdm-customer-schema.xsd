								<jxb:property>
									<jxb:javadoc>The customer's chosen password. Must conform to
										the password policy.
									</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="pin" type="xs:string" minOccurs="0">
